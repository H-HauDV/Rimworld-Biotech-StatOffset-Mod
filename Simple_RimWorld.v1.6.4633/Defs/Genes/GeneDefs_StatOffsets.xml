    <labelShortAdj>Combat</labelShortAdj>
    <statFactors>
      <ArrestSuccessChance>10</ArrestSuccessChance>
      <MeleeCooldownFactor>0.05</MeleeCooldownFactor>
      <MeleeDamageFactor>24</MeleeDamageFactor>
      <IncomingDamageFactor>0.01</IncomingDamageFactor>
      <StaggerDurationFactor>0</StaggerDurationFactor>
      <PawnTrapSpringChance>-1</PawnTrapSpringChance>
      <ShootingAccuracyFactor_Long>10</ShootingAccuracyFactor_Long>
      <ShootingAccuracyFactor_Medium>10</ShootingAccuracyFactor_Medium>
      <ShootingAccuracyFactor_Short>10</ShootingAccuracyFactor_Short>
      <ShootingAccuracyFactor_Touch>10</ShootingAccuracyFactor_Touch>
      <MortarMissRadiusFactor>0.05</MortarMissRadiusFactor>
    </statFactors>
    <statOffsets>
      <!-- Armor -->
      <ArmorRating_Sharp>72</ArmorRating_Sharp>
      <ArmorRating_Blunt>72</ArmorRating_Blunt>
      <ArmorRating_Heat>72</ArmorRating_Heat>
      <!-- Shooting -->
      <AimingDelayFactor>-0.99</AimingDelayFactor>
      <RangedCooldownFactor>-0.99</RangedCooldownFactor>
      <ShootingAccuracyPawn>12</ShootingAccuracyPawn>
      <ShootingAccuracyOutdoorsDarkOffset>12</ShootingAccuracyOutdoorsDarkOffset>
      <ShootingAccuracyOutdoorsLitOffset>12</ShootingAccuracyOutdoorsLitOffset>
      <ShootingAccuracyIndoorsDarkOffset>12</ShootingAccuracyIndoorsDarkOffset>
      <ShootingAccuracyIndoorsLitOffset>12</ShootingAccuracyIndoorsLitOffset>
      <!-- Meele hit change -->
      <MeleeArmorPenetration>16</MeleeArmorPenetration>
      <MeleeHitChance>100</MeleeHitChance>
      <MeleeHitChanceOutdoorsDarkOffset>12</MeleeHitChanceOutdoorsDarkOffset>
      <MeleeHitChanceOutdoorsLitOffset>12</MeleeHitChanceOutdoorsLitOffset>
      <MeleeHitChanceIndoorsDarkOffset>12</MeleeHitChanceIndoorsDarkOffset>
      <MeleeHitChanceIndoorsLitOffset>12</MeleeHitChanceIndoorsLitOffset>
      <!-- Dodge chance -->
      <MeleeDodgeChance>100</MeleeDodgeChance>
      <MeleeDodgeChanceOutdoorsDarkOffset>12</MeleeDodgeChanceOutdoorsDarkOffset>
      <MeleeDodgeChanceOutdoorsLitOffset>12</MeleeDodgeChanceOutdoorsLitOffset>
      <MeleeDodgeChanceIndoorsDarkOffset>12</MeleeDodgeChanceIndoorsDarkOffset>
      <MeleeDodgeChanceIndoorsLitOffset>12</MeleeDodgeChanceIndoorsLitOffset>
    </statOffsets>
  </GeneDef>

