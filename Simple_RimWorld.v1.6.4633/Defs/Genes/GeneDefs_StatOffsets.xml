  <!-- Work -->
  <GeneDef>
    <displayCategory>CharaStatOffsetAdjuster</displayCategory>
    <description>No description.</description>
    <biostatCpx>0</biostatCpx>
    <biostatMet>-1</biostatMet>
    <selectionWeight>0</selectionWeight>
    <displayOrderInCategory>2</displayOrderInCategory>
    <iconPath>UI/Icons/Genes/Gene_NakedSpeed</iconPath>
    <defName>CharaStatOffsetAdjuster_Work</defName>
    <label>Work stat gene</label>
    <labelShortAdj>Work</labelShortAdj>
    <statFactors>
      <TameAnimalChance>16</TameAnimalChance>
      <TrainAnimalChance>16</TrainAnimalChance>
      <DrugHarvestYield>5</DrugHarvestYield>
      <AnimalGatherYield>5</AnimalGatherYield>
      <ButcheryFleshEfficiency>5</ButcheryFleshEfficiency>
      <ButcheryMechanoidEfficiency>5</ButcheryMechanoidEfficiency>
      <SocialIdeoSpreadFrequencyFactor>128</SocialIdeoSpreadFrequencyFactor>
    </statFactors>
    <statOffsets>
      <!-- General work speed -->
      <WorkSpeedGlobal>32</WorkSpeedGlobal>
      <WorkTableWorkSpeedFactor>6</WorkTableWorkSpeedFactor>
      <GeneralLaborSpeed>1</GeneralLaborSpeed>
      <!-- Specific work speed -->
      <ConstructionSpeed>2</ConstructionSpeed>
      <DrugSynthesisSpeed>1</DrugSynthesisSpeed>
      <SmoothingSpeed>1</SmoothingSpeed>
      <PruningSpeed>2</PruningSpeed>
      <SmeltingSpeed>2</SmeltingSpeed>
      <PlantWorkSpeed>0.5</PlantWorkSpeed>
      <DrugCookingSpeed>4</DrugCookingSpeed>
      <DeepDrillingSpeed>2</DeepDrillingSpeed>
      <MiningSpeed>2</MiningSpeed>
      <ResearchSpeedFactor>72</ResearchSpeedFactor>
      <AnimalGatherSpeed>0.5</AnimalGatherSpeed>
      <CookSpeed>24</CookSpeed>
      <CleaningSpeed>64</CleaningSpeed>
      <ReadingSpeed>16</ReadingSpeed>

      <ConstructSuccessChance>1</ConstructSuccessChance>
      <FixBrokenDownBuildingSuccessChance>1</FixBrokenDownBuildingSuccessChance>
      <MiningYield>12</MiningYield>
      <PlantHarvestYield>12</PlantHarvestYield>
      <HuntingStealth>32</HuntingStealth>
      <!-- Medical work -->
      <SurgerySuccessChanceFactor>12</SurgerySuccessChanceFactor>
      <MedicalSurgerySuccessChance>12</MedicalSurgerySuccessChance>
      <MedicalOperationSpeed>2</MedicalOperationSpeed>
      <MedicalTendSpeed>2</MedicalTendSpeed>
      <MedicalTendQuality>12</MedicalTendQuality>
      <!-- Learning -->
      <GlobalLearningFactor>48</GlobalLearningFactor>
      <LearningRateFactor>32</LearningRateFactor>
      <!-- Economy -->
      <ConversionPower>16</ConversionPower>
      <DrugSellPriceImprovement>12</DrugSellPriceImprovement>
      <AnimalProductsSellImprovement>12</AnimalProductsSellImprovement>
      <NegotiationAbility>24</NegotiationAbility>
      <TradePriceImprovement>12</TradePriceImprovement>
      <SocialImpact>36</SocialImpact>
    </statOffsets>
  </GeneDef>

