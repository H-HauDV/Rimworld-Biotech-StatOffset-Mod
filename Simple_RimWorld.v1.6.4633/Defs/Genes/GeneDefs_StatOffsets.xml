<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <GeneCategoryDef>
    <defName>CharaStatOffsetAdjuster</defName>
    <label>Stat offset adjuster</label>
    <displayPriorityInXenotype>995</displayPriorityInXenotype>
  </GeneCategoryDef>

  <!-- Utility -->
  <GeneDef>
    <displayCategory>CharaStatOffsetAdjuster</displayCategory>
    <description>No description.</description>
    <biostatCpx>0</biostatCpx>
    <biostatMet>-1</biostatMet>
    <selectionWeight>0</selectionWeight>
    <displayOrderInCategory>0</displayOrderInCategory>
    <iconPath>UI/Icons/Genes/Gene_NonSenescent</iconPath>
    <defName>CharaStatOffsetAdjuster_Utility</defName>
    <label>Utility stat gene</label>
    <labelShortAdj>Utility</labelShortAdj>
    <statFactors>
      <Flammability>0</Flammability>
      <RawNutritionFactor>24</RawNutritionFactor>
      <LifespanFactor>100</LifespanFactor>
      <CertaintyLossFactor>0.1</CertaintyLossFactor>
      <JoyFallRateFactor>0.05</JoyFallRateFactor>
      <FoodPoisonChance>0</FoodPoisonChance>
      <CancerRate>0</CancerRate>
    </statFactors>
    <statOffsets>
      <FilthRate>-0.99</FilthRate>
      <!-- Carrying -->
      <CarryingCapacity>1000</CarryingCapacity>
      <!-- Moving -->
      <MoveSpeed>48</MoveSpeed>
      <CrawlSpeed>12</CrawlSpeed>
      <!-- Healing -->
      <InjuryHealingFactor>500</InjuryHealingFactor>
      <!-- Enviroment resistance -->
      <ToxicResistance>12</ToxicResistance>
      <ToxicEnvironmentResistance>12</ToxicEnvironmentResistance>
      <ComfyTemperatureMin>-1200</ComfyTemperatureMin>
      <ComfyTemperatureMax>1800</ComfyTemperatureMax>
      <ImmunityGainSpeedFactor>12</ImmunityGainSpeedFactor>
      <ImmunityGainSpeed>12</ImmunityGainSpeed>
      <VacuumResistance MayRequire="Ludeon.RimWorld.Odyssey">1</VacuumResistance>
      <!-- Mood -->
      <MentalBreakThreshold>-0.999</MentalBreakThreshold>
      <PainShockThreshold>1</PainShockThreshold>
      <!-- Rest sleep -->
      <RestRateMultiplier>12</RestRateMultiplier>
      <RestFallRateFactor>-0.95</RestFallRateFactor>
      <!-- Beauty -->
      <PawnBeauty>64</PawnBeauty>
      <!-- Hunger -->
      <MaxNutrition>35</MaxNutrition>
      <ForagedNutritionPerDay>48</ForagedNutritionPerDay>
      <EatingSpeed>4</EatingSpeed>
      <BedHungerRateFactor>-0.99</BedHungerRateFactor>

    </statOffsets>
  </GeneDef>

